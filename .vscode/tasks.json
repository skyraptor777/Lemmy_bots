{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "markdownlint",
			"problemMatcher": [
				"$markdownlint"
			],
			"label": "markdownlint: Lint all Markdown files in the workspace with markdownlint",
			"group": "build"
		},
		{
			"type": "typescript",
			"tsconfig": "tsconfig.json",
			"option": "watch",
			"problemMatcher": [
				"$tsc-watch"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "tsc: watch - tsconfig.json"
		}
	]
}